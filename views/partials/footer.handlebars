<footer class="container footer">
  <div class="row">
    {{#if settings.footer_menu}}
    <nav class="column">
      {{#each settings.footer_menu}}
        <a href="{{this.url}}">{{this.name}}</a>{{#unless @last}} |{{/unless}}
      {{/each}}
    </nav>
    {{/if}}
    <div class="column">
      &copy;{{ settings.year }} - {{ settings.authorName }}
    </div>
  </div>
</footer>

